&spmi_bus {
	qcom,pm8916@0 {
		//ASUS_BSP Austin_T : Enable LED driver +++
		qcom,leds@a100 {
			status = "okay";
			qcom,led_mpp_2 {
				label = "mpp";
				linux,name = "green";
				linux,default-trigger = "none";
				qcom,default-state = "off";
				qcom,max-current = <5>;
				qcom,current-setting = <5>;
				qcom,id = <6>;
				qcom,mode = "pwm";
				qcom,source-sel = <8>;
				qcom,mode-ctrl = <0x60>;
				pwms = <&pm8916_pwm 0 0>;
				qcom,pwm-us = <100>; //setup 0.0001s pwm output
			};
		};
		qcom,leds@a300 {
			status = "okay";
			qcom,led_mpp_4 {
				label = "mpp";
				linux,name = "red";
				linux,default-trigger = "none";
				qcom,default-state = "off";
				qcom,max-current = <5>;
				qcom,current-setting = <5>;
				qcom,id = <6>;
				qcom,mode = "pwm";
				qcom,source-sel = <8>;
				qcom,mode-ctrl = <0x60>;
				pwms = <&pm8916_pwm 0 0>;
				qcom,pwm-us = <100>;  //setup 0.0001s pwm output
			};
		};
		//ASUS_BSP Austin_T : Enable LED driver ---
	};
};

